{
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "^1.12",
        "phpstan/extension-installer": "^1.4",
        "thecodingmachine/phpstan-strict-rules": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
